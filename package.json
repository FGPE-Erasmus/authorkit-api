{
  "name": "fgpe-authorkit-api",
  "version": "1.0.0",
  "description": "FGPE AuthorKit API - The API of a toolkit to author gamified activities to support programming learning. This is part of the Erasmus+ Project entitled Framework for Gamified Programming Education (FGPE).",
  "main": "dist/index.js",
  "repository": "git@github.com:josepaiva94/fgpe-authorkit-api.git",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node -r ts-node/register -r dotenv-safe/config src/index.ts",
    "cli": "node -r ts-node/register -r dotenv-safe/config src/cli.ts",
    "watch:dev": "nodemon",
    "watch:debug": "nodemon --config nodemon-debug.json",
    "prebuild": "rm -rf ./dist",
    "build": "tsc",
    "postbuild": "cp -R ./src/assets ./dist",
    "prod": "node -r dotenv-safe/config dist/index.js",
    "lint:check": "tslint --project ./",
    "lint:fix": "tslint --project ./ --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "ts-typeorm": "ts-node ./node_modules/typeorm/cli.js"
  },
  "author": "Jos√© C. Paiva <josepaiva94@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 8"
  },
  "precommit": [
    "lint:check"
  ],
  "dependencies": {
    "@nestjs/bull": "0.0.3",
    "@nestjs/common": "^6.11.8",
    "@nestjs/core": "^6.11.8",
    "@nestjs/graphql": "^6.5.5",
    "@nestjs/microservices": "^6.11.8",
    "@nestjs/passport": "^6.2.0",
    "@nestjs/platform-express": "^6.11.8",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.3.3",
    "@nestjs/websockets": "^6.11.8",
    "@nestjsx/crud": "^4.4.1",
    "@nestjsx/crud-typeorm": "^4.4.1",
    "@octokit/rest": "^16.43.1",
    "accesscontrol": "^2.2.1",
    "accesscontrol-re": "^3.1.0",
    "apollo-server-express": "^2.10.1",
    "archiver": "^3.1.1",
    "async": "^2.6.2",
    "async-exit-hook": "^2.0.1",
    "bull": "^3.13.0",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "dotenv-safe": "^5.0.1",
    "email-templates": "^6.1.1",
    "express-rate-limit": "^5.1.1",
    "faker": "^4.1.0",
    "graphql": "^14.6.0",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tag-pluck": "^0.8.7",
    "graphql-tools": "^4.0.7",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "luxon": "^1.22.0",
    "nestjs-command": "^1.3.0",
    "node-cache": "^5.1.0",
    "nodemailer": "^5.1.1",
    "passport": "^0.4.1",
    "passport-facebook-token": "^3.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.18.2",
    "pug": "^2.0.4",
    "qs-middleware": "^1.0.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.5.4",
    "swagger-ui-express": "^4.1.3",
    "tslib": "^1.11.0",
    "type-graphql": "^0.17.6",
    "typeorm": "^0.2.22",
    "unzipper": "^0.10.9",
    "winston": "^2.4.4",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.11.8",
    "@types/archiver": "^3.0.0",
    "@types/async": "^2.4.1",
    "@types/bull": "^3.12.0",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.9",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/lodash": "^4.14.149",
    "@types/luxon": "^1.21.0",
    "@types/node": "^10.17.16",
    "@types/node-cache": "^4.2.5",
    "@types/nodemailer": "^4.6.6",
    "@types/redis": "^2.8.16",
    "@types/socket.io": "^2.1.2",
    "@types/unzipper": "^0.10.2",
    "@types/xml2js": "^0.4.5",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "pre-commit": "^1.2.2",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.20.1",
    "typescript": "^3.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
