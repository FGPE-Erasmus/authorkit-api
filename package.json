    "passport": "^0.4.1",
    "passport-facebook-token": "^4.0.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.3.0",
    "pug": "^3.0.0",
    "qs-middleware": "^1.0.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.2",
    "rxjs-compat": "^6.6.2",
    "swagger-ui-express": "^4.1.4",
    "translation-google": "^0.2.1",
    "tslib": "^2.0.1",
    "typeorm": "^0.2.25",
    "unzipper": "^0.10.11",
    "uuid": "^8.3.0",
