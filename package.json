    "@types/bull": "^3.14.0",
    "@types/express": "^4.17.7",
    "@types/faker": "^4.1.12",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.159",
    "@types/luxon": "^1.24.3",
    "@types/node": "^14.0.27",
    "@types/node-cache": "^4.2.5",
    "@types/nodemailer": "^6.4.0",
    "@types/redis": "^2.8.25",
    "@types/socket.io": "^2.1.11",
    "@types/unzipper": "^0.10.3",
    "@types/uuid": "^8.3.0",
    "@types/xml2js": "^0.4.5",
    "jest": "^26.3.0",
    "nodemon": "^2.0.4",
    "pre-commit": "^1.2.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
