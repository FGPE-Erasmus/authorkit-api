{
  "name": "fgpe-authorkit-api",
  "version": "1.0.0",
  "description": "FGPE AuthorKit API - The API of a toolkit to author gamified activities to support programming learning. This is part of the Erasmus+ Project entitled Framework for Gamified Programming Education (FGPE).",
  "main": "dist/index.js",
  "repository": "git@github.com:josepaiva94/fgpe-authorkit-api.git",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node -r ts-node/register -r dotenv-safe/config src/index.ts",
    "cli": "node -r ts-node/register -r dotenv-safe/config src/cli.ts",
    "watch:dev": "nodemon",
    "watch:debug": "nodemon --config nodemon-debug.json",
    "prebuild": "rm -rf ./dist",
    "build": "npx tsc",
    "postbuild": "cp -R ./src/assets ./dist",
    "prod": "node -r dotenv-safe/config dist/index.js",
    "lint:check": "tslint --project ./",
    "lint:fix": "tslint --project ./ --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "ts-typeorm": "ts-node ./node_modules/typeorm/cli.js"
  },
  "author": "Jos√© C. Paiva <josepaiva94@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 8"
  },
  "precommit": [
    "lint:check"
  ],
  "resolutions": {
    "crc32-stream": "^4.0.2"
  },
  "overrides": {
    "crc32-stream": "^4.0.2"
  },
  "dependencies": {
    "@nestjs/bull": "0.1.2",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^9.0.5",
    "@nestjs/microservices": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^4.5.12",
    "@nestjs/typeorm": "^7.0.0",
    "@nestjs/websockets": "^7.0.0",
    "@nestjsx/crud": "4.6.2",
    "@nestjsx/crud-typeorm": "4.6.2",
    "@octokit/rest": "^18.0.0",
    "apollo-server-express": "^2.16.1",
    "archiver": "^5.3.0",
    "async": "^3.2.0",
    "async-exit-hook": "^2.0.1",
    "bull": "^3.16.0",
    "class-transformer": "0.3.1",
    "class-validator": "^0.12.2",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "email-templates": "^7.1.0",
    "express-rate-limit": "^5.1.3",
    "faker": "^4.1.0",
    "helmet": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "luxon": "^1.24.1",
